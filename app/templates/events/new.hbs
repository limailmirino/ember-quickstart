<h3>Insert a new event</h3>


<p><label for="title">Title:</label>{{input type="text" name="title" value=model.title size="32"}}</p>
<p><label for="location">Location:</label>{{input type="text" name="location" value=model.location size="32"}}</p>
<p><label for="start">Start date:</label>{{input type="text" name="start" value=model.start size="32"}}</p>
<p><label for="end">End date:</label>{{input type="text" name="end" value=model.end size="32"}}</p>
<p>
  <label for="description">Description:</label>
  {{textarea name="description" value=model.description cols="80" rows="6"}}
</p>

<button class="btn btn-default" {{action "saveEvent" model}} >Submit</button>


